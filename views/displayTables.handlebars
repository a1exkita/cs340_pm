<body>

    <header>
      <!-- The <i> tag below includes the bullhorn icon from Font Awesome -->
      <a href="./"><h1 class="site-title">Project Management Tool</h1></a>

      <nav class="navbar">
        <ul class="navlist">
          <li class="navitem navlink"><a href="./insert">Insert a project</a></li>
          <li class="navitem navlink"><a href="./addDepartment">Add a department</a></li>
          <li class="navitem navlink"><a href="./addProgrammer">Add a programmer</a></li>
	  <li class="navitem navlink"><a href="./displayTables">Display Employees and Departments</a></li>

        </ul>
      </nav>
    </header>

    <main>
	
    <h3>Current Employees:</h3>
    <table>
    	<thead>
		<th>ID</th>
		<th>Name</th>
		<th>Manager</th>
		<th>Department</th>
	</thead>
	<tbody>
		{{#each employees}}
		<tr>
			<td>{{ID}}</td>
			<td>{{Name}}</td>
			<td>{{Manager}}</td>
			<td>{{Department}}</td>
		</tr>
		{{/each}}
	</tbody>
    </table>

    <h3>Current Departments:</h3>
    <table>
    	<thead>
		<th>ID</th>
		<th>Name</th>
	</thead>
	<tbody>
		{{#each departments}}
		<tr>
			<td>{{ID}}</td>
			<td>{{Name}}</td>
		</tr>
		{{/each}}
	</tbody>
    </table>


    </main>

  </body>

</html>
